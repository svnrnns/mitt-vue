{
  "name": "mitt-vue",
  "version": "2.1.3",
  "description": "Lightweight utility for integrating mitt with Vue 2 and Vue 3",
  "license": "MIT",
  "type": "module",
  "main": "dist/mitt-vue.cjs",
  "module": "dist/mitt-vue.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/mitt-vue.js",
    "require": "./dist/mitt-vue.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svnrnns/mitt-vue"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "event-emitter",
    "mitt",
    "event-management",
    "vue-events",
    "event-subscription",
    "event-handling",
    "vue-components",
    "mitt-integration",
    "event-listener",
    "vue-utility",
    "vue-event-emitter",
    "event-emitters-in-vue",
    "vue-hook-for-events",
    "event-management-library",
    "vue-event-handling",
    "vue-event-system",
    "event-bus-vue",
    "vue-event-listener",
    "vue2-event-handling",
    "vue3-event-handling"
  ],
  "bugs": {
    "url": "https://github.com/svnrnns/mitt-vue/issues"
  },
  "homepage": "https://github.com/svnrnns/mitt-vue",
  "author": "seven rings (https://twitter.com/svnrnns)",
  "dependencies": {
    "mitt": "3.0.1",
    "vue": "^2.6.0 || ^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6",
    "typescript": "^5.7.2",
    "vite": "^7",
    "vite-plugin-dts": "^2.3.0"
  }
}
